{
  "version": 2,
  "buildCommand": "chmod +x vercel-build.sh && ./vercel-build.sh",
  "devCommand": "npm run dev",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/output/:path*",
      "destination": "/public/output/:path*"
    }
  ],
  "functions": {
    "api/stories.js": {
      "memory": 128,
      "maxDuration": 5
    }
  },
  "github": {
    "silent": true
  }
} 